<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ibm.erp.value">

    <class name="ProductValue" table="products">
        <id name="id" >
        </id>
        <property name="name"/>
        <property name="price"/>
        <property name="unit"/>
        <property name="qty"/>
        <property name="buynum"/>
        
    </class>

</hibernate-mapping>
