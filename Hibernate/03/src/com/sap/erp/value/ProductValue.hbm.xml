<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.sap.erp.value">

    <class name="ProductValue" table="sap_product">
        <id name="pno" type="int">
           <generator class="increment"/> 
        </id>
        <property name="pcode" ></property>
        <property name="pname" ></property>
        <property name="pdate" ></property>
        <property name="edate"></property>
        <property name="unitprice"></property>
        <property name="qtyinstock"></property>
        
        
    </class>

</hibernate-mapping>
